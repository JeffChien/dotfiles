{
  "$schema": "https://opencode.ai/config.json",
  // Theme configuration
  "theme": "dracula",
  "model": "github_copilot/grok-code-fast-1",
  "small_model": "github_copilot/gpt-4.1",
  "provider": {
    "litellm": {
      "npm": "@ai-sdk/openai-compatible",
      "name": "litellm",
      "options": {
        "baseURL": "https://litellm.service.huinn.cc/v1",
        "apiKey": "{file:~/.credentials/llm/litellm}"
      },
      "models": {
        "github_copilot/gpt-4.1": {},
        "github_copilot/gpt-5-mini": {},
        "github_copilot/raptor-mini": {},
        "github_copilot/grok-code-fast-1": {}
      }
    }
  },
  "keybinds": {
    "input_submit": "ctrl+return",
    "input_newline": "return,ctrl+j"
  }
}
