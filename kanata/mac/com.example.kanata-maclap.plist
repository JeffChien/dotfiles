<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.example.kanata-maclap</string>

    <key>ProgramArguments</key>
    <array>
        <string>/run/current-system/sw/bin/kanata</string>
        <string>-c</string>
        <string>/Users/jchien/dotfiles/kanata/nix/keyboard-maclap.kbd</string>
    </array>

    <key>RunAtLoad</key>
    <true/>

    <key>KeepAlive</key>
    <true/>

    <key>StandardOutPath</key>
    <string>/Library/Logs/Kanata/kanata-maclap.out.log</string>

    <key>StandardErrorPath</key>
    <string>/Library/Logs/Kanata/kanata-maclap.err.log</string>
</dict>
</plist>